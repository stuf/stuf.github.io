// Place your settings in this file to overwrite default and user settings.
{
    "files.associations": {
        "*.html": "liquid"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.sass-cache": true,
        "**/.jekyll-metadata": true
    }
}